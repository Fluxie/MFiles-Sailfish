; EditorConfig is awesome: http://EditorConfig.org

; top-most EditorConfig file
root = true

; Force 4-character tab indent on JS, QML and C++ files
[*]
charset = utf-8
trim_trailing_whitespace = true

[*.js]
indent_style = tab
trim_trailing_whitespace = true

[*.qml]
indent_style = tab
trim_trailing_whitespace = true

[*.cpp]
indent_style = tab
trim_trailing_whitespace = true

[*.h]
indent_style = tab
trim_trailing_whitespace = true

